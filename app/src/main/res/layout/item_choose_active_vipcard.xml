<?xml version="1.0" encoding="utf-8"?><!--车辆列表子项-->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:layout_marginTop="20px"
              android:background="#fafafa"
              android:orientation="horizontal">

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:background="@color/white"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:padding="@dimen/common_padding2">

            <TextView
                android:id="@+id/tv_vipcard_type_no"
                android:layout_width="wrap_content"
                android:layout_height="26px"
                android:layout_centerVertical="true"
                android:background="@drawable/bg_vipcard_no"
                android:gravity="center"
                android:maxLines="1"
                android:paddingLeft="10px"
                android:paddingRight="10px"
                android:text="000"
                android:textColor="@color/white"
                android:textSize="22px"/>

            <TextView
                android:id="@+id/tv_vipcard_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20px"
                android:layout_toRightOf="@id/tv_vipcard_type_no"
                android:maxLines="1"
                android:text="xxx测试卡"
                android:textColor="@color/dark_gray_text"
                android:textSize="32px"
                android:textStyle="bold"/>

            <TextView
                android:id="@+id/tv_vipcard_type"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:maxLines="1"
                android:text="次卡"
                android:textColor="@color/dark_gray_text"
                android:textSize="26px"/>
        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:orientation="horizontal"
            android:paddingLeft="@dimen/common_padding2"
            android:paddingRight="@dimen/common_padding2">

            <TextView
                android:id="@+id/tv_service_content"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="64px"
                android:layout_weight="1"
                android:ellipsize="end"
                android:gravity="center_vertical"
                android:maxLines="1"
                android:text="可用服务"
                android:textColor="@color/light_gray_text"
                android:textSize="@dimen/little_font_size"/>

            <TextView
                android:id="@+id/tv_show_more"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:maxLines="1"
                android:text="详情"
                android:textColor="#1e88e5"
                android:textSize="@dimen/little_font_size"/>
        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:padding="@dimen/common_padding2">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/label_face_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:maxLines="1"
                    android:text="面值："
                    android:textColor="@color/dark_gray_text"
                    android:textSize="26px"/>

                <TextView
                    android:id="@+id/tv_face_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20px"
                    android:maxLines="1"
                    android:text="0次"
                    android:textColor="@color/dark_gray_text"
                    android:textSize="26px"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:maxLines="1"
                    android:text="有效年限："
                    android:textColor="@color/dark_gray_text"
                    android:textSize="26px"/>

                <TextView
                    android:id="@+id/tv_original_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20px"
                    android:maxLines="1"
                    android:text="不限"
                    android:textColor="@color/dark_gray_text"
                    android:textSize="26px"/>

            </LinearLayout>
        </RelativeLayout>
    </LinearLayout>

    <ImageView
        android:id="@+id/iv_checked"
        android:layout_width="110px"
        android:layout_height="match_parent"
        android:scaleType="center"
        android:src="@drawable/ic_unchecked3"/>
</LinearLayout>